
# bart_public_ip with ip address of bart server

# db_public_ip with ip address of master database server

# username with user name on system.

# path_to_file with path of ssh private key file.

#### Sample file input ###

### 172.30.1.0 ansible_user=centos ansible_ssh_private_key_file=/Users/Documents/abc.pem
### 172.30.1.1 ansible_user=centos ansible_ssh_private_key_file=/Users/Documents/abc.pem



############# Replace value here  #######

bart_public_ip ansible_user=username  ansible_ssh_private_key_file=path_to_file

db_public_ip ansible_user=username ansible_ssh_private_key_file=path_to_file

